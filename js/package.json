    "babel-preset-react-app": "^9.1.2",
    "css-loader": "^1.0.1",
    "pnp-webpack-plugin": "^1.6.4",
    "react-dev-utils": "^10.2.1",
    "resolve": "^1.17.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.1 || ^2 || ^3",
    "lodash": "^4.17.4",
    "pubsub-js": "^1.9.0",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "vitessce": "^1.1.9"
  },
  "jupyterlab": {
