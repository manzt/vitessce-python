{
  "name": "vitessce-jupyter",
  "version": "0.1.3",
  "description": "Jupyter widget facilitating interactive visualization of spatial single-cell data with Vitessce",
  "author": "Mark Keller",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/vitessce/vitessce-jupyter.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab",
    "jupyterlab-extension"
